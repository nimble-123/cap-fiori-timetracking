{
  "name": "timetable",
  "version": "0.0.1",
  "description": "Fiori Elements List Report for capturing and reviewing project time entries via annotations.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "build": "ui5 build preload --clean-dest --config ui5-deploy.yaml",
    "build-local": "ui5 build preload --clean-dest",
    "deploy-config": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf",
    "start": "ui5 serve",
    "wdi5": "wdio run ./webapp/test/e2e/wdio.conf.ts"
  },
  "devDependencies": {
    "@openui5/types": "^1.142.0",
    "@sap-ux/eslint-plugin-fiori-tools": "^0.6.1",
    "@sap/ux-ui5-tooling": "^1.19.3",
    "@sapui5/types": "^1.141.3",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.3",
    "@ui5/cli": "^4.0.33",
    "@wdio/cli": "^9.20.0",
    "@wdio/local-runner": "^9.20.0",
    "@wdio/mocha-framework": "^9.20.0",
    "@wdio/spec-reporter": "^9.20.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "ui5-task-zipper": "^3",
    "ui5-tooling-transpile": "^3.9.2",
    "wdio-ui5-service": "^3.0.0"
  },
  "ui5": {
    "dependencies": [
      "ui5-task-zipper"
    ]
  }
}
